{
    "version":2,
 
    "builds":[
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/api/v1/(.*)",
            "dest":"/"
        }
    ],
     "headers": [
          {
            "source": "/api/v1/(.*)",
            "headers": [
              { "key": "Access-Control-Allow-Credentials", "value": "true" }
            ]
          }
        ]
}